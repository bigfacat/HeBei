<%@ Page Language="C#" AutoEventWireup="true" CodeBehind="TxsqView.aspx.cs" Inherits="JlueTaxSystemHBGS.wszx_web.apps.views.fydj.TxsqView" %>

<!DOCTYPE html>


<h2>填写申请表</h2>
<section class="first-step">
    <h5 class="title">复业（提前复业）报告书</h5>
    <table cellpadding="0" cellspacing="0" border="0">
        <tr>
            <td class="search-item">纳税人识别号：</td>
            <td class="search-item-control" id="nsrsbh"></td>
            <td class="search-item">纳税人名称：</td>
            <td class="search-item-control" id="nsrmc"></td>
        </tr>
        <tr>
            <td class="search-item" width="7%">报告日期：</td>
            <td class="search-item-control">
                <input class="mini-datepicker" id="bgrq" name="bgrq" required="true" width="100%"></td>
            <td class="search-item">复业日期：</td>
            <td class="search-item-control">
                <input class="mini-datepicker" id="fydj" name="fydj" required="true" width="100%"></td>
        </tr>
        <tr>
            <td class="search-item">停业期限起：</td>
            <td>
                <input class="mini-datepicker" id="tyrqStart" name="tyrqStart" required="true" width="100%"></td>
            <td class="search-item">停业期限止：</td>
            <td class="search-item-control">
                <input class="mini-datepicker" id="tyrqEnd" name="tyrqEnd" required="true" width="100%"></td>
        </tr>
    </table>
    <h5>领回税务资料情况</h5>
    <div id="lhswzlqk" class="mini-datagrid" style="width: 1160px; height: 210px;" allowresize="false" showpager="false" showemptytext="true">
        <div property="columns">
            <div field="swzjzlMc" headeralign="center">税务证件种类</div>
            <div field="swzjhm" headeralign="center">税务证件号码</div>
        </div>
    </div>
    <h5>领回其他税务资料情况</h5>
    <div id="qtzl" class="mini-datagrid" style="width: 1160px; height: 210px;" allowresize="false" showpager="false" showemptytext="true">
        <div property="columns">
            <div field="jcqtswzlmc" headeralign="center">其他税务资料名称</div>
            <div field="jcqtswzlsl" headeralign="center">数量</div>
        </div>
    </div>
    <h5>领回发票情况</h5>
    <div id="lhfp" class="mini-datagrid" style="width: 1160px; height: 210px;" allowresize="false" showpager="false" showemptytext="true">
        <div property="columns">
            <div field="jhfpzlDm" headeralign="center">发票种类</div>
            <div field="fpDm" headeralign="center">发票代码</div>
            <div field="fpqshm" headeralign="center">发票起始号码</div>
            <div field="fpzzhm" headeralign="center">发票终止号码</div>
            <div field="fpsl" headeralign="center">数量</div>
        </div>
    </div>
</section>
