html, body {
  margin: 0; }

.container {
  width: 1200px;
  margin: 0 auto;
  padding: 20px 0;
  overflow-x: auto; }

table {
  border-collapse: collapse;
  border-spacing: 0;
  margin: 0 auto; }
  table.border th, table.border td {
    border: 1px solid #000;
    padding: 5px;
    word-break: break-all; }

.nowrap {
  white-space: nowrap !important; }

/*btn-group*/
.btn-group {
  text-align: center;
  padding: 20px;
  background: #fff; }

/*button*/
.btn {
  margin: 0 4px;
  display: inline-block;
  padding: 6px 27px;
  font-size: 14px;
  font-weight: normal;
  text-decoration: none;
  cursor: pointer;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  background-image: none;
  border: 1px solid transparent;
  border-radius: 4px; }
  .btn:hover {
    text-decoration: none; }
  .btn.hidden {
    display: none; }
  .btn.btn-gray {
    color: #fff;
    background: #cdd5d7; }
    .btn.btn-gray:hover, .btn.btn-gray.active {
      background: #b8bfc1; }
  .btn.btn-blue {
    color: #fff;
    background: #0c84db; }
    .btn.btn-blue:hover, .btn.btn-blue.active {
      background: #0b77c5; }
  .btn.btn-orange {
    color: #fff;
    background: #fba207; }
    .btn.btn-orange:hover, .btn.btn-orange.active {
      background: #e19206; }
  .btn.btn-red {
    color: #fff;
    background: #d3433c; }
    .btn.btn-red:hover, .btn.btn-red.active {
      background: #b42c26; }
  .btn.btn-green {
    color: #fff;
    background: #86b558; }
    .btn.btn-green:hover, .btn.btn-greenactive {
      background: #6c9742; }

.tip {
  width: 1200px;
  margin: 0 auto; }

