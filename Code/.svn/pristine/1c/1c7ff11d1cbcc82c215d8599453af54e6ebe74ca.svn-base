.context-box {
    width: 1200px;
    height: 513px;
    margin: 0 auto;
    overflow-x: hidden;
    overflow-y: auto;
    background: #fff;
    text-align: center;
    border: 1px solid #ebebeb;
    border-radius: 5px;
}

    .context-box .leftMenu {
        display: inline-block;
        float: left;
        width: 18%;
        height: 100%;
        background: #FAFBFD;
        border-right: 1px solid #EEE;
    }

        .context-box .leftMenu a {
            font-size: 16px;
            text-align: center;
            color: #848484;
            float: left;
            width: 100%;
            padding: 10px 0;
            border-bottom: 1px solid #EEE;
        }

            .context-box .leftMenu a.active {
                font-weight: 700;
                color: #1096DD;
                background-color: #fff !important;
                border-radius: 2px;
                border-left: 3px solid #0A94DD;
            }

            .context-box .leftMenu a.creditBtn {
                background: url(../../images/creditGrade/credit.png) 40px 8px no-repeat;
            }

            .context-box .leftMenu a.active {
                background: url(../../images/creditGrade/creditActive.png) 40px 8px no-repeat;
            }

            .context-box .leftMenu a.riskBtn {
                background: url(../../images/creditGrade/risk.png) 40px 8px no-repeat;
            }

                .context-box .leftMenu a.riskBtn.active {
                    background: url(../../images/creditGrade/riskActive.png) 40px 8px no-repeat;
                }

            .context-box .leftMenu a.personBtn {
                background: url(../../images/creditGrade/personCredit.png) 40px 8px no-repeat;
            }

                .context-box .leftMenu a.personBtn.active {
                    background: url(../../images/creditGrade/personCreditActive.png) 40px 8px no-repeat;
                }

    .context-box .creditMenu {
        float: right;
        width: 78%;
        height: 510px;
        overflow-x: hidden;
        overflow-y: auto;
    }

        .context-box .creditMenu::-webkit-scrollbar {
            margin-left: -5px;
            width: 7px;
            height: 7px;
        }

        .context-box .creditMenu::-webkit-scrollbar-track {
            background-color: #ddd;
            border-radius: 3px;
        }

        .context-box .creditMenu::-webkit-scrollbar-thumb {
            background-color: #aaa;
            border-radius: 3px;
        }

        .context-box .creditMenu .radar-chart {
            margin-top: 30px;
            height: 300px;
        }

        .context-box .creditMenu .influ-title {
            font-size: 18px;
            padding: 15px 0;
            border-bottom: 1px #ddd solid;
            color: #333;
        }

        .context-box .creditMenu .creditCon, .context-box .creditMenu .personCon {
            text-align: left;
            padding-bottom: 30px;
        }

            .context-box .creditMenu .creditCon .influ-item, .context-box .creditMenu .personCon .influ-item {
                color: #333;
                margin-top: 20px;
                font-size: 16px;
                background: url(../../images/creditGrade/ico-credit-arrow.png) 0 4px no-repeat;
                text-indent: 15px;
            }

            .context-box .creditMenu .creditCon .influ-break-line, .context-box .creditMenu .personCon .influ-break-line {
                height: 3px;
                background: url(../../images/creditGrade/ico-break-dot.png);
                margin: 20px 0;
            }

            .context-box .creditMenu .creditCon ul, .context-box .creditMenu .personCon ul {
                overflow: hidden;
                margin: 5px;
            }

                .context-box .creditMenu .creditCon ul li, .context-box .creditMenu .personCon ul li {
                    float: left;
                    margin-right: 72px;
                    text-indent: -5px;
                    color: #666;
                }

                    .context-box .creditMenu .creditCon ul li.no-dot, .context-box .creditMenu .personCon ul li.no-dot {
                        list-style: none;
                    }

                        .context-box .creditMenu .creditCon ul li.no-dot a, .context-box .creditMenu .personCon ul li.no-dot a {
                            color: #0A94DD;
                        }

                            .context-box .creditMenu .creditCon ul li.no-dot a:hover, .context-box .creditMenu .personCon ul li.no-dot a:hover {
                                text-decoration: underline;
                            }

            .context-box .creditMenu .creditCon ul {
                padding-left: 15px;
            }

                .context-box .creditMenu .creditCon ul li {
                    float: none;
                    list-style: none;
                    margin: 7px 0;
                }

            .context-box .creditMenu .creditCon #syqys ul li, .context-box .creditMenu .creditCon #xyqys ul li {
                list-style: decimal;
                margin-left: 5px;
            }
