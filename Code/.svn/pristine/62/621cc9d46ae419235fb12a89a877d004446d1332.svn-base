/*河北 侧边栏*/
.sidebar-hb {
    position: fixed;
    bottom: 30%;
    right: 0;
    width: 45px;
    font-size: 12px;
    font-family: "MicroSoft YaHei";
    z-index: 100;
}
.sidebar-hb .item{
    display: flex;
    width: 45px;
    height: 45px;
    background-color: #099adc;
    border-bottom: 1px #f2f4f8 solid;
    justify-content: center;
    align-items: center;
    cursor: pointer;
}
.sidebar-hb .item:hover{
    background-color: #099adc;
}
.sidebar-hb .item:hover .qrCode-tips{
    display: block;
}
.sidebar-hb .item-top {
    border-radius: 0 0 0 5px;
    background: #999 url('../images/sidebar/sidebar-top.png') center center no-repeat;
    background-size: 26px 26px;
}

.sidebar-hb .item-qrCode {
    border-radius: 5px 0 0 0;
    background: #999 url('../images/sidebar/sidebar-ewm.png') center center no-repeat;
    background-size: 26px 26px;
}

.sidebar-hb .item-qrCode .qrCode-tips {
    position: absolute;
    left: -150px;
    top: -20px;
    display: none;
}

.sidebar-hb .item-qrCode .qrCode-tips img {
    width: 140px;
    height: 140px;
}

.txt-c{
    text-align: center;
}
.sidebar-hb .item-video {
    background: #999 url('../images/sidebar/sidebar-video.png') center center no-repeat;
    background-size: 26px 26px;
}

.sidebar-hb .item-help {
    background: #999 url('../images/sidebar/sidebar-help.png') center center no-repeat;
    background-size: 26px 26px;
}

.sidebar-hb .item-feedback {
    background: #999 url("../images/sidebar/sidebar-feedback.png") center center no-repeat;
    background-size: 26px 26px;
}