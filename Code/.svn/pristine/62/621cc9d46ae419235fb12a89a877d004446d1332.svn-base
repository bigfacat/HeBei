/** public css */
body {
    width: 100%;
    min-width: 1280px;
}
.txt-blue{
    color: #0994dc;
}
.mt10{
    margin-top: 10px;
}
.ft14 {
    font-size: 14px;
}

.fl {
    float: left;
}

.fr {
    float: right;
}

.hide {
    display: none !important;
}

.qrcode {
    height: 40px;
    float: right;
    border: 0px;
}

/* 清除浮动 */
.clearfix {
    *zoom: 1;
}

.clearfix:before,
.clearfix:after {
    display: table;
    line-height: 0;
    content: "";
}

.clearfix:after {
    clear: both;
}

.inline-block {
    display: inline-block;
}

.block {
    display: block;
}

.hidden {
    display: none !important;
}

.font-color3 {
    color: #333333;
}

.font-color6 {
    color: #666666;
}

.font-color9 {
    color: #999999;
}

.mt20 {
    margin-top: 20px;
}

.mb20 {
    margin-bottom: 20px;
}

.mr20 {
    margin-right: 20px;
}

input[type='button'] {
    font-family: 微软雅黑;
}

.button {
    border: none;
    cursor: pointer;
}

.button-blue {
    color: #ffffff;
    background: #0994dc;
}

.button-blue:hover {
    background: #0974bc;
}

.button-grey {
    background: #f2f2f2;
    border: 1px #d9d9d9 solid;
}

.button-grey:hover {
    background: #e8e8e8;
}

/** 用户管理 main.css */
.yhgl-wrap {
    width: 1200px;
    margin: 0 auto;
    position: relative;
}

.app2wm-content {
    display: inline-block;
    float: right;
    width: 190px;
    text-align: right;
    height: 80px;
}

.app2wm-content img {
    display: inline-block;
    width: 80px;
}

/** 登陆 login.css*/
.login-bg {
    background: url(../images/login-bg.png) no-repeat center 80px;
    min-width: 1280px;
    z-index: -1;
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 540px;

}

.header {
    width: 1200px;
    margin: 0 auto;
    height: 55px;
    line-height: 55px;
    background: url(../images/logos.png) no-repeat 0 5px;
}
.yhgl-login{
    background: url(../images/login-bg.png) no-repeat center center;
    background-size: 100% 100%;
    min-width: 1280px;
    width: 100%;
}
.yhgl-login .content {
    overflow: hidden;
    height: 350px;
    width: 1200px;
    margin: 0 auto;

}

.yhgl-login .content .box {
    background-color: #fff;
    height: 300px;
    width: 300px;
    padding: 0 20px;
    position: relative;
    margin-top: 10px;
}

.yhgl-login .content .box .login-box {
    display: table-cell;
    vertical-align: middle;
    height: inherit;
    width: inherit;
    position: relative;
}
.yhgl-login .content .box .login-box .tab-list{
    border-bottom: 1px #ddd solid;
    text-align: center;
}
.yhgl-login .content .box .login-box .tab-item{
    margin-top: 15px;
    height: 205px;
    min-height: 205px;
    text-align: center;
}
.yhgl-login .content .box .login-box .tab-list li{
    display: inline-block;
    padding: 10px 35px;
    cursor: pointer;
}
.yhgl-login .content .box .login-box .tab-list li.active{
    color: #fff;
    background-color: #0994dc;
    border-radius: 5px 5px 0 0;
}

.yhgl-login .content .box .login-box h3 {
    text-align: center;
}

.yhgl-login .content .box .login-box form {
    text-align: center;
    overflow: hidden;
}

.yhgl-login .content .box .login-box input[type='text'], .yhgl-login .content .box .login-box input[type='password'] {
    width: 100%;
    height: 40px;
    line-height: 40px;
    margin-bottom: 15px;
    font-size: 15px;
    box-sizing: border-box;
    font-family: "MicroSoft YaHei";
    text-indent: 3.5em;
    border-radius: 4px;
    border: 1px #d9d9d9 solid;
}
.yhgl-login .content .box .login-box .ico-for-name,
.yhgl-login .content .box .login-box .ico-for-pwd,
.yhgl-login .content .box .login-box .ico-for-code{
    width: 42px;
    height: 38px;
    display: inline-block;
    position: absolute;
    left: 1px;
    border-right: 1px solid #d9d9d9;
    border-radius: 5px 0 0 5px;
}
.yhgl-login .content .box .login-box .ico-for-name{
    background: url(../images/ico-username.png) #fafafa center center no-repeat;
    top: 75px;
}
.yhgl-login .content .box .login-box .ico-for-pwd{
    background: url(../images/ico-password.png) #fafafa center center no-repeat;
    top: 130px;
}
.yhgl-login .content .box .login-box .ico-for-code{
    background: url(../images/ico-verification.png) #fafafa center center no-repeat;
    top: 185px;
}

.yhgl-login .content .box .login-box input[type='button'], .yhgl-login .content .box .login-box a {
    display: inline-block;
    height: 40px;
    vertical-align: middle;
    font-size: 16px;
    border-radius: 4px;
}

.yhgl-login .content .box .login-box a {
    line-height: 40px;
}

.yhgl-login .content .box .login-box .login-btn {
    width: 168px;
}

.yhgl-login .content .box .login-box .reg-btn {
    width: 112px;
    color: #666666;
    margin-left: 8px;
    height: 38px;
    line-height: 38px;
}

.yhgl-login .content .box .help {
    text-align: center;
    font-size: 12px;
    position: absolute;
    bottom: -40px;
    right: -33px;
    color: #fff;
    width: 370px;
}

.yhgl-login .content .box .help a {
    color: #fff;
    cursor: pointer;
    font-size: 14px;
}

.yhgl-login .content .box .help a:hover {
    text-decoration: underline;
}
.yhgl-wrap .footer{
    margin: 10px 0;
}
.yhgl-wrap .footer ul {
    text-align: center;
}

.yhgl-wrap .footer ul > li {
    display: inline-block;
    padding-right: 20px;
    vertical-align: middle;
}

.yhgl-wrap .footer ul > li a {
    color: #666666;
    font-size: 14px;
}

.yhgl-wrap .footer ul > li a:hover {
    text-decoration: underline;
}

/** 对话框业务组件 dialog.css*/
.dialog-tab {
    min-height: 320px;
}

.dialog-tab .tab {
    width: 116px;
    padding-top: 10px;
    min-height: inherit;
    background-color: #F6F7F8;
}

.dialog-tab .tab li {
    text-align: center;
    cursor: pointer;
    color: #666;
    font-size: 16px;
    padding: 14px 4px;
}

.dialog-tab .tab li.actived {
    background-color: #ffffff;
    color: #0994dc;
}

.dialog-tab .item-list {
    position: relative;
    left: 50%;
    margin-left: -209px;
    display: table;
    min-height: inherit;
}

.dialog-tab .item-list #choiceCompany {
    height: 192px;
    overflow: auto;
    width: 343px;
}

.dialog-tab .item-list div {
    display: table-cell;
    vertical-align: middle;
    padding: 10px 0;
}

.dialog-tab .item-list div ul > li {
    border: 1px solid #d9d9d9;
    cursor: pointer;
    border-radius: 6px;
    padding: 10px;
    font-size: 14px;
    color: #666666;
    margin-bottom: 10px;
    width: 280px;
    text-align: center;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.dialog-tab .item-list div ul > li.choiced {
    border-color: #0994dc;
    color: #0994dc;
    background: url(../images/choiced.png) no-repeat right bottom;
}

.dialog-tab .item-list div p {
    height: 80px;
    background: url(../images/personal.png) no-repeat center center;
}

.dialog-tab .item-list div ol > li {
    list-style: none;
    text-align: center;
    color: #666666;
    font-size: 14px;
    height: 30px;
    line-height: 30px;
}

.dialog-tab .item-list div a {
    display: block;
    margin-top: 20px;
    width: 302px;
    height: 40px;
    line-height: 40px;
    text-align: center;
    font-size: 16px;
    border-radius: 4px;
}

.dialog-tab .item-list div a#addCompany {
    /* background: url(../images/add.png) 75px 3px no-repeat; */
    float: right;
    width: 150px;
    color: #2FBAE8;
    font-weight: bold;
    font-size: 14px;
    margin-right: 30px;
    margin-top: 0;
}

.dialog-tab .item-list a.refresh {
    float: left;
    color: #2FBAE8;
    font-weight: bold;
    font-size: 14px;
    margin-right: 30px;
    margin-top: 0;
    width: 85px;
    cursor: pointer;
}

.dialog-tab .addContent {
    display: block;
    margin: 0;
    list-style: none;
}

.dialog-tab .addContent li {
    float: left;
    width: 320px;
    height: 35px;
    line-height: 35px;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    cursor: pointer;
    text-align: center;
    border-radius: 5px;
    margin: 10px 0 10px 100px;
    border: 2px solid #169BD5;
}

.dialog-tab .addContent a {
    float: left;
    width: 85px;
    height: 55px;
    margin-left: 20px;
    line-height: 55px;
}

.dialog-tab .addContent a:-webkit-any-link {
    text-decoration: none;
}

.dialog-tab .addContent a.underLine {
    color: #169BD5;
}

.dialog-tab .addContent a.underLine:hover {
    text-decoration: underline;
}

.dialog-type {
    width: 660px;
    margin: 0 auto;
    font-size: 0;
    height: 280px;
    padding-left: 20px;
}

.dialog-type h3 {
    font-size: 18px;
    text-align: center;
    color: #666666;
    margin: 20px 0 20px 0;
}

.dialog-type div {
    box-sizing: border-box;
    cursor: pointer;
    display: inline-block;
    font-size: 14px;
    width: 300px;
    background-color: #F6F7F8;
    border-radius: 8px;
    padding: 20px;
    -moz-box-shadow: 1px 1px 2px #888888;
    /* 老的 Firefox */
    box-shadow: 1px 1px 2px #888888;
}

.dialog-type div:hover {
    background-color: #f1f1f1;
}

.dialog-type div h2 {
    height: 50px;
    line-height: 50px;
    text-indent: 60px;
}

.dialog-type div.normal > h2 {
    background: url(../images/normal.png) no-repeat 0 center;
}

.dialog-type div.byh > h2 {
    background: url(../images/byh.png) no-repeat 0 center;
}

.dialog-type div p {
    color: #666666;
    line-height: 24px;
}

.dialog-type div p span {
    color: #0994dc;
}

/* 遮罩层 */
.mask {
    height: 700px;
    width: 100%;
    z-index: 99;
    position: absolute;
    top: 0;
    bottom: 0;
    background-color: #000;
    opacity: 0.9;
}

/*// .mask .mask-left,.mask .mask-right {
//   max-width: 360px;
//   width: 100%;
//   background-color: #000;
//   opacity: 0.9;
// }*/
.mask .mask-content {
    width: 1200px;
    height: inherit;
    margin: 0 auto;
    z-index: 100;
/ / display: inline-block;
}

.mask .mask-content div {
/ / display: inline-block;
    cursor: pointer;
}

.mask .mask-first {
    height: inherit;
    background: url(../images/one-step.png) 100% no-repeat;
    background-position: 547px 312px;
/ / display: none;
}

.mask .mask-second {
    height: inherit;
    background: url(../images/two-step.png) 100% no-repeat;
    background-position: 318px 209px;
    display: none;
}

.mask .mask-third {
    height: inherit;
    background: url(../images/three-step.png) 100% no-repeat;
    background-position: 350px 368px;
    display: none;
}

.mask .mask-forth {
    height: inherit;
    background: url(../images/four-steps.png) 100% no-repeat;
    background-position: 516px 380px;
    display: none;
}

/* 授权帮助 */
