}

.company-input {
    border: 1px solid #ccc;
    width: 300px;
    height: 27px;
    text-indent: 5px;
    border-radius: 3px;
}

.app-mask {
    position: absolute;
    display: none;
    top: 110px;
    z-index: 4;
    width: 1200px;
    right: 18px;
    height: 380px;
}
.ico-mask-2wm{
    position: absolute;
    right: 700px;
    top: -100px;
}
.ico-mask-text{
    position: absolute;
    right: 420px;
    top: -30px;
}
.ico-mask-arrow{
    top: -130px;
    position: absolute;
    right: 200px;
}
.ico-mask-app{
    position: absolute;
    right: 90px;
    top: -88px;
}
.ico-mask-close{
    position: absolute;
    right: 20px;
