}
.real-mask{
    position: fixed;
    display: none;
    width: 100%;
    height: 100%;
    background-color: rgba(0,0,0,0.8);
    z-index: 3;
    top: 0;
}

.ggfw-content{
    background-color: #f7f7f7;
    overflow: hidden;
    position: relative;
}

.ggfw-content .tzgg-content {
    height: 70px;
    background: #fff;
    overflow: hidden;
    margin-top: 10px;
    border: 1px solid #ebebeb;
}

.ggfw-content .tzgg-content div {
    display: inline-block;
}

.ggfw-content .tzgg-content .tzgg-title {
    font-size: 26px;
    color: #333;
    background: #eee;
    padding: 0 40px;
    height: 70px;
    line-height: 70px;
    float: left;
}

.ggfw-content .tzgg-content .tzgg-list {
    float: left;
    font-size: 14px;
    height: 60px;
    overflow: hidden;
}

.ggfw-content .tzgg-content .tzgg-ul {
    list-style: none;
    margin: 0;
    line-height: 26px;
    padding: 9px 35px;
}
.ggfw-content .tzgg-content .tzgg-ul .tzgg-item{
    width: 880px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}
.ggfw-content .tzgg-content .tzgg-triangle {
    background: url(../images/idx_txt.png) 14px 22px no-repeat,url(../images/idx_notice.png) top left no-repeat;
    width: 170px;
    height: 70px;
    float: left;
    color: #0092db;
    cursor: pointer;
}
.ggfw-content .tzgg-content .tzgg-triangle span{
    display: block;
    width: 100px;
    margin: 17px 50px;
}
.ggfw-content .tzgg-content .tzgg-triangle span i{
    font-style: normal;
}

.ggfw-content .tzgg-content .pg-up-down {
    float: right;
    height: 40px;
    overflow: hidden;
    padding: 15px 20px;
}

.ggfw-content .tzgg-content .pg-up-down .pg-up {
    display: block;
    background: url("../images/icon_up01.png") center center no-repeat;
    width: 24px;
    height: 20px;
    cursor: pointer;
}

.ggfw-content .tzgg-content .pg-up-down .pg-down {
    display: block;
    background: url("../images/icon_down01.png") center center no-repeat;
    width: 24px;
    height: 20px;
    cursor: pointer;
}

.ggfw-content .tzgg-content .pg-up-down .pg-up:hover {
    background: url("../images/icon_up02.png") center center no-repeat;
}

.ggfw-content .tzgg-content .pg-up-down .pg-down:hover {
    background: url("../images/icon_down02.png") center center no-repeat;
}
.ggfw-content .wkc-ul{
