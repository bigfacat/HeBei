    margin-top: 10px;
    border:1px #ebebeb solid;
    background-color: #fff;
    width: 700px;
}
.ggfw-content .ggfw-title{
    height: 45px;
    line-height: 45px;
    font-size: 16px;
    border-bottom: 1px #ebebeb solid;
