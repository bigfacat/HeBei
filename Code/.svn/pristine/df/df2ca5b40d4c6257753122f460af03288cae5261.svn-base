.header-area{height:110px;background:#ffffff;margin-bottom:40px}.header-area .home-logo{background:url("../images/ico-logo-home.png") 0 0 no-repeat;width:329px;height:41px;margin:5px 0}.header-area .change-account{color:#666666;margin-top:9px}.header-area .change-account span{margin-right:20px}.header-area .change-account .ico-avatar{display:inline-block;width:100px;height:34px;color:#0994dc;background:url("../images/ico-avatar.png") left center no-repeat;text-align:right;line-height:34px}.header-area .modules-area{height:60px;color:#ffffff;background:#0994dc}.header-area .modules-area ul{padding:0;margin:0;list-style:none}.header-area .modules-area ul .bar{width:100%;background:#0994dc;margin-top:-8px}.header-area .modules-area ul li{font-size:16px;line-height:60px;float:left;padding:0 25px;cursor:pointer}.header-area .modules-area ul li.active{overflow:visible;background:#0085d1 url("../images/ico-up-sj.png") center 51px no-repeat}.header-area .header-container{width:1200px;margin:0 auto;overflow:hidden}.header-area .header-container .breadcrumb-Nav,.header-area .header-container .company-info{height:60px;line-height:60px;display:inline-block;color:#fff;font-size:14px}.header-area .header-container .breadcrumb-Nav span,.header-area .header-container .company-info span{cursor:pointer}.header-area .header-container .breadcrumb-Nav a,.header-area .header-container .company-info a{color:#fff;text-decoration:none}
