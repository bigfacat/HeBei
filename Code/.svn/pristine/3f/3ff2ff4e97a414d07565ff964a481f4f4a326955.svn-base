.top-line-div{
    width: 950px;
    margin: auto;
    line-height: 35px;
}

.top-line-left{
    float: left;
    width: 480px;
    font-size: 12px;
    text-align: left;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.top-line-left img{
    float: left;
}

.top-line-right{
    width: auto;
    height: 26px;
    line-height: 26px;
    float: right;
    margin-top: 5px;
    margin-right: 15px;
    font-size: 14px;
    right: 10px;
}

.top-line-right li{
    height: 26px;
    float: left;
    display: inline-block;
}

.top-line-btn-xg,
.top-line-btn-dy,
.top-line-btn-fh,
.top-line-btn-zf{
    width: 81px;
}

.top-line-btn-xg a,
.top-line-btn-zf a,
.top-line-btn-dy a,
.top-line-btn-fh a {
    width: 81px;
    height: 26px;
    display: block;
    padding-left: 12px;
    color: #333333;
}

.top-line-btn-xg a {
    background-image: url(../images/xg1.jpg);
}
.top-line-btn-xg a:hover{
    background-image: url(../images/xg2.jpg);
}
.top-line-btn-zf a{
    background-image: url(../images/zf1.jpg);
}
.top-line-btn-zf a:hover{
    background-image: url(../images/zf2.jpg);
}
.top-line-btn-dy a{
    background-image: url(../images/dy1.jpg);
}
.top-line-btn-dy a:hover{
    background-image: url(../images/dy2.jpg);
}
.top-line-btn-fh a{
    background: url(../images/fh1.jpg) no-repeat;
}
.top-line-btn-fh a:hover{
    background: url(../images/fh2.jpg) no-repeat;
}
.top-line-btn-sm{
    width: 92px;
}
.top-line-btn-sm a{
    width: 92px;
    height: 26px;
    display: block;
    background: url(../images/tbsm1.jpg) no-repeat;
    padding-left: 12px;
